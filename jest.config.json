{
  "preset": "react-native",
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 75,
      "lines": 80,
      "statements": 60
    }
  },
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!src/**/actions/**/*.{js,jsx}"
  ],
  "coverageReporters": [
    "clover",
    "json",
    "lcov",
    "text",
    "cobertura"
  ]
}