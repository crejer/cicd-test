{
  "name": "myawesomeproject",
  "version": "1.0.4",
  "private": true,
  "scripts": {
    "translate": "i18next 'src/**/*.js' 'src/components/**/*.js' 'src/**/*.ts' 'src/**/*.tsx'",
    "postinstall": "patch-package",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --ci --reporters=default --reporters=jest-junit",
    "lint": "eslint src --fix",
    "prettier": "prettier src --write",
    "prepare": "husky install",
    "set-version": "react-native-version --never-amend --skip-tag --never-increment-build"
  },
  "dependencies": {
    "@os-team/i18next-react-native-language-detector": "^1.0.16",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@reduxjs/toolkit": "^1.8.5",
    "appcenter": "^4.4.5",
    "appcenter-analytics": "^4.4.5",
    "appcenter-crashes": "^4.4.5",
    "i18next": "^21.9.2",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-react-native-async-storage": "^1.0.4",
    "immer": "^9.0.15",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.1.0",
    "react-i18next": "^11.18.6",
    "react-native": "0.70.1",
    "react-native-version": "^4.0.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "^11.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "husky": "^8.0.0",
    "i18next-parser": "^6.5.0",
    "jest": "^26.6.3",
    "jest-junit": "^14.0.1",
    "metro-react-native-babel-preset": "^0.72.1",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/actions/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "clover",
      "json",
      "lcov",
      "text",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}
